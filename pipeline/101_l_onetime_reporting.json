{
	"name": "101_l_onetime_reporting",
	"properties": {
		"activities": [
			{
				"name": "101_l_unique_carriers",
				"description": "ik laad de l_unique carriers data in van csv naar de database.",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 2,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"additionalColumns": [
							{
								"name": "initial_source",
								"value": "$$FILEPATH"
							}
						],
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFileName": "950256511_T_ONTIME*.csv"
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Code",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "code",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "Description",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "description",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "initial_source",
									"type": "String"
								},
								"sink": {
									"name": "initial_source",
									"type": "String",
									"physicalType": "varchar"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "csv_onetime_reporting",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "l_unique_carriers",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "100_STG_katrien"
		},
		"annotations": []
	}
}